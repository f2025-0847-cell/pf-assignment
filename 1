#include <iostream>
using namespace std;

int main() {
    float m1, m2, m3, m4, m5, total, percent;
    cout << "Enter marks for 5 subjects: ";
    cin >> m1 >> m2 >> m3 >> m4 >> m5;
    total = m1 + m2 + m3 + m4 + m5;
    // Assuming each subject is out of 100, percentage = (total / 500) * 100
    percent = (total / 500) * 100;
    cout << "Total: " << total << endl;
    cout << "Percentage: " << percent << endl;
    if (percent >= 90)
        cout << "Grade: A+" << endl;
    else if (percent >= 80)
        cout << "Grade: A" << endl;
    else if (percent >= 70)
        cout << "Grade: B" << endl;
    else if (percent >= 60)
        cout << "Grade: C" << endl;
    else if (percent >= 50)
        cout << "Grade: D" << endl;
    else
        cout << "Grade: F" << endl;
    if (percent >= 50)
        cout << "Passed" << endl;
    else
        cout << "Failed" << endl;
    return 0;
}